# name: test/sql/mssql/14_datetime2.test
# description: test for DATETIME2 type with MSSQL
# group: [mssql_14_datetime2]

require odbc_scanner

require icu

statement ok
SET VARIABLE conn = odbc_connect('ODBCSCANNER_DEBUG_CONN_STRING_ENV_VAR=ODBC_CONN_STRING')

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(''2020-12-31 23:58:59.1234567'' AS DATETIME2)')
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(''2020-12-31 23:58:59.1234567'' AS DATETIME2)',
  datetime2_columns_as_timestamp_ns=TRUE)
----
2020-12-31 23:58:59.1234567

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(''2020-12-31 23:58:59.1234567'' AS DATETIME2)')
)
----
TIMESTAMP

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(''2020-12-31 23:58:59.1234567'' AS DATETIME2)',
    datetime2_columns_as_timestamp_ns=TRUE)
)
----
TIMESTAMP_NS

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('2020-12-31 23:58:59.1234567'::TIMESTAMP_NS))
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('2020-12-31 23:58:59.1234567'::TIMESTAMP_NS),
  datetime2_columns_as_timestamp_ns=TRUE)
----
2020-12-31 23:58:59.1234567

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('2020-12-31 23:58:59.123456789'::TIMESTAMP_NS))
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('2020-12-31 23:58:59.123456789'::TIMESTAMP_NS),
  datetime2_columns_as_timestamp_ns=TRUE)
----
2020-12-31 23:58:59.1234567

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2(3))', params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 23:58:59.123

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('0001-01-01 00:00:00.000000'::TIMESTAMP))
----
0001-01-01 00:00:00

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('0001-01-01 00:00:00.000000'::TIMESTAMP),
  datetime2_columns_as_timestamp_ns=TRUE)
----
-infinity

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('9999-12-31 23:59:59.999999'::TIMESTAMP))
----
9999-12-31 23:59:59.999999

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params=row('9999-12-31 23:59:59.999999'::TIMESTAMP),
  datetime2_columns_as_timestamp_ns=TRUE)
----
infinity

statement ok
SET VARIABLE params1 = odbc_create_params()

statement ok
PREPARE p1 AS SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATETIME2)', params_handle=getvariable('params1'))

statement ok
PREPARE p1_bind AS SELECT odbc_bind_params(getvariable('conn'), getvariable('params1'), row(?))

statement ok
EXECUTE p1_bind('2020-12-31 23:58:59.123456'::TIMESTAMP)

query I
EXECUTE p1
----
2020-12-31 23:58:59.123456

statement ok
EXECUTE p1_bind('2020-12-31 23:58:59.1234567'::TIMESTAMP_NS)

query I
EXECUTE p1
----
2020-12-31 23:58:59.123456

statement ok
EXECUTE p1_bind('1970-01-01 00:00:00'::TIMESTAMP)

query I
EXECUTE p1
----
1970-01-01 00:00:00

statement ok
EXECUTE p1_bind('1969-12-31 00:00:00'::TIMESTAMP)

query I
EXECUTE p1
----
1969-12-31 00:00:00

statement ok
DEALLOCATE p1_bind

statement ok
DEALLOCATE p1

