# name: test/sql/snowflake/07_date.test
# description: test for DATE type with Snowflake
# group: [snowflake_07_date]

require odbc_scanner

statement ok
SET VARIABLE conn = odbc_connect('ODBCSCANNER_DEBUG_CONN_STRING_ENV_VAR=ODBC_CONN_STRING')

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT to_date(''2020-12-31'', ''YYYY-MM-DD'')')
----
2020-12-31

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT to_date(''9999-12-31'', ''YYYY-MM-DD'')')
----
9999-12-31

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'), 'SELECT to_date(''2020-12-31'', ''YYYY-MM-DD'')')
)
----
DATE

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATE)', params=row('2020-12-31'::DATE))
----
2020-12-31

statement ok
SET VARIABLE params1 = odbc_create_params()

statement ok
PREPARE p1 AS SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS DATE)', params_handle=getvariable('params1'))

statement ok
PREPARE p1_bind AS SELECT odbc_bind_params(getvariable('conn'), getvariable('params1'), row(?))

statement ok
EXECUTE p1_bind('2020-12-31'::DATE)

query I
EXECUTE p1
----
2020-12-31

statement ok
EXECUTE p1_bind('1970-01-01'::DATE)

query I
EXECUTE p1
----
1970-01-01

statement ok
EXECUTE p1_bind('1969-12-31'::DATE)

query I
EXECUTE p1
----
1969-12-31

