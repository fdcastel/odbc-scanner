# name: test/sql/snowflake/10_timestamp_ltz.test
# description: test for Snowflake TIMESTAMP_LTZ type
# group: [snowflake_10_timestamp_ltz]

require odbc_scanner

statement ok
SET VARIABLE conn = odbc_connect('ODBCSCANNER_DEBUG_CONN_STRING_ENV_VAR=ODBC_CONN_STRING')

statement ok
CALL odbc_query(getvariable('conn'), 'ALTER SESSION SET TIMEZONE = ''Australia/Darwin''')

query I
SELECT * FROM odbc_query(getvariable('conn'),
  'SELECT to_timestamp_ltz(''2020-12-31 12:34:56.123456789-05:00'', ''YYYY-MM-DD HH24:MI:SS.FF9TZH:TZM'')')
----
2020-12-31 17:34:56.123456

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'),
    'SELECT to_timestamp_ltz(''2020-12-31 12:34:56.123456789-05:00'', ''YYYY-MM-DD HH24:MI:SS.FF9TZH:TZM'')')
)
----
TIMESTAMP

query I
SELECT * FROM odbc_query(getvariable('conn'),
  'SELECT to_timestamp_ltz(''2020-12-31 12:34:56.123456789-05:00'', ''YYYY-MM-DD HH24:MI:SS.FF9TZH:TZM'')',
  timestamp_columns_as_timestamp_ns=TRUE)
----
2020-12-31 17:34:56.123456789

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'),
    'SELECT to_timestamp_ltz(''2020-12-31 12:34:56.123456789-05:00'', ''YYYY-MM-DD HH24:MI:SS.FF9TZH:TZM'')',
    timestamp_columns_as_timestamp_ns=TRUE)
)
----
TIMESTAMP_NS

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP_LTZ)',
  params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 14:28:59.123456

statement ok
SET VARIABLE params1 = odbc_create_params()

statement ok
PREPARE p1 AS SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP_LTZ)',
  params_handle=getvariable('params1'))

statement ok
PREPARE p1_bind AS SELECT odbc_bind_params(getvariable('conn'), getvariable('params1'), row(?))

statement ok
EXECUTE p1_bind('2020-12-31 23:58:59.123456'::TIMESTAMP)

query I
EXECUTE p1
----
2020-12-31 14:28:59.123456

