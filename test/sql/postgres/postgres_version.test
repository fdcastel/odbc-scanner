# name: test/sql/postgres/postgres_version.test
# description: test for PostgreSQL version
# group: [sql_postgres_version]

require odbc_scanner

require-env POSTGRES_AVAILABLE

statement ok
SET VARIABLE conn = odbc_connect('ODBCSCANNER_DEBUG_CONN_STRING_ENV_VAR=ODBC_CONN_STRING')

statement ok
CALL odbc_query(getvariable('conn'), 'SELECT version()')

statement ok
SELECT odbc_close(getvariable('conn'))
