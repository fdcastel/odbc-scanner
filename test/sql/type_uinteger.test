# name: test/sql/type_uinteger.test
# description: test for UINTEGER type
# group: [sql_type_uinteger]

require odbc_scanner

statement ok
SET VARIABLE conn = odbc_connect('Driver={DuckDB Driver};')

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT 42::UINTEGER')
----
42

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT ?::UINTEGER', params=row(42::UINTEGER))
----
42

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT ?::UINTEGER', params=row(0::UINTEGER))
----
0

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT ?::UINTEGER', params=row(4294967295::UINTEGER))
----
4294967295

statement ok
SET VARIABLE params1 = odbc_create_params()

statement ok
PREPARE p1 AS SELECT * FROM odbc_query(getvariable('conn'), 'SELECT ?::UINTEGER', params_handle=getvariable('params1'))

statement ok
PREPARE p1_bind AS SELECT odbc_bind_params(getvariable('params1'), row(?))

statement ok
EXECUTE p1_bind(4294967295::UINTEGER)

query I
EXECUTE p1
----
4294967295

statement ok
DEALLOCATE p1_bind

statement ok
DEALLOCATE p1

statement ok
SELECT odbc_close(getvariable('conn'))
